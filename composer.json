{
    "name": "afromessage/afromessage-laravel",
    "description": "Laravel SDK for AfroMessage SMS and OTP services",
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.10",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "illuminate/support": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.36",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Afromessage\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Afromessage\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Afromessage\\Laravel\\AfroMessageServiceProvider"
            ],
            "aliases": {
                "AfroMessage": "Afromessage\\Laravel\\Facades\\AfroMessage"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
